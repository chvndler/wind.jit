{
  "name": "wind.jit",
  "version": "0.1.0",
  "main": "index.html",
  "license": "MIT",
  "keywords": [
    "static",
    "template",
    "codesandbox"
  ],
  "scripts": {
    "serve": "serve -l 3000 -s src",
    "start": "live-server --port=6006 --entry-file=src/index.html",
    "template": "live-server --port=6006 --entry-file=src/template.html",
    "dev": "live-server --port=3000",
    "pretty": "prettier --write \"src/**/*.html\""
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "serve": "^14.2.0"
  }
}
